{
	"root": true,
	"env": {
		"node": true,
		"browser": true
	},
	"rules": {
		"no-console": 0,
		"no-cond-assign": 0,
		"no-unused-vars": 1
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/essential"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
	    "parser": "babel-eslint",
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module"
	}
}
